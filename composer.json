{
    "name": "kba-team/cakephp-predis",
    "description": "Predis cache engine for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kamilwylegala/cakephp2-php8"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer/installers": "^1.0",
        "cakephp/cakephp": "dev-master",
        "predis/predis": "^1.1",
        "ext-redis": "*"
    },
    "extra": {
        "installer-name": "PredisCache"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
