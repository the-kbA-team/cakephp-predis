{
    "name": "kba-team/cakephp-predis",
    "description": "Predis cache engine for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0,<9",
        "cakephp/cakephp": "^5.0",
        "predis/predis": "^1.1",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
            "kbATeam\\CakePhpPredis\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    }
}
